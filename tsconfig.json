{
  "compilerOptions": {
    "noEmit": true,
    "module": "esnext",
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "types": [
      "node",
      "@cloudflare/workers-types"
    ]
  },
  "exclude": [
    "node_modules",
    "dist",
    "test"
  ],
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  }
}
